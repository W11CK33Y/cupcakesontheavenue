{
  "version": 2,
  "routes": [
    {
      "src": "/api/create-checkout",
      "methods": ["POST"],
      "dest": "/api/create-checkout.js"
    }
  ],
  "functions": {
    "api/create-checkout.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "SUMUP_API_KEY": "@sumup_api_key",
    "SUMUP_API_SECRET": "@sumup_api_secret"
  }
}